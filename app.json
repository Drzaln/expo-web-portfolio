{
  "expo": {
    "name": "Doddy Rizal N",
    "slug": "expo-portfolio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#212529",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDFFFC"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FDFFFC"
      }
    },
    "web": {
      "name": "Doddy",
      "favicon": "./assets/favicon.png",
      "display": "standalone",
      "themeColor": "#212529",
      "backgroundColor": "#212529",
      "startUrl": "/",
      "scope": "/",
      "orientation": "portrait",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#FDFFFC"
      }
    }
  }
}
